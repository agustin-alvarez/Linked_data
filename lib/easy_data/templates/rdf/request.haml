<?xml version="1.0" encoding="utf-8"?>

%rdf:RDF={"xmlns:rdf"=>"http://www.w3.org/1999/02/22-rdf-syntax-ns#"}.merge(@rdf_model[:header])
  = @rdf_model[:body].each do |element|
    %rdf:Description{:about => element.about}
      = element.attributes.each do |att,value|
        %att
          = puts value
      = end
  = end



